air.hittime = ceil(cond(22-Map(HitstunScaling) < 5, 5, 22-Map(HitstunScaling)))
air.fall = 1
fall.yvelocity = 0
guard.ctrltime = 13
guard.hittime = 13
ground.cornerpush.veloff = -4
p2facing = 1

